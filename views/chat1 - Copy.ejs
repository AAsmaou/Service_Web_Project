						<input type="text" class="message_form_input" name="message" id="input" value="" placeholder="Press Enter to send">
					</form>
			</div>
			<div class="modal-footer">
				<!-- <button type="button" class="btn btn-primary" data-dismiss="modal">Close</button> -->
			</div>
